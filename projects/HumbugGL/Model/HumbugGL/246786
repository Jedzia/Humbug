      operation 161538 "TextPosition"
	preserve_cpp_body_indent public explicit_return_type ""
	nparams 5
	  param in name "text" explicit_type "string"
	  param in name "position" explicit_type "int"
	  param in name "linenr" explicit_type "int"
	  param in name "column" explicit_type "int"
	  param in name "delimiter" explicit_type "string"
	    defaultvalue "\":\""
	cpp_decl "    ${comment}${inline}${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}, ${t2} ${p2}${v2}, ${t3} ${p3}${v3}, ${t4} ${p4}${v4}${)}${volatile}${throw};
"
	cpp_def "${comment}${inline}${class}::${name}${(}${t0} ${p0}, ${t1} ${p1}, ${t2} ${p2}, ${t3} ${p3}, ${t4} ${p4}${)}
: text(${p0}), length(${p0}.length()), position(${p1}), linenr(${p2}), column(${p3}), delimiter(${p4})
, mName(findName(${p0})) , mModifier(findModifier(${p0})) ${throw}{
  ${body}}
"
	
	
	
	
	oid 161538
	comment "! Creates a Textposition object.
/ \\param text Template text.
/ \\param position The position (char number) inside the TextDocument.
/ \\param linenr The line containing the text inside theTextDocument.
/ \\param column The column of the text inside theTextDocument.
/ \\param delimiter A char that separates the template text from the 
/ template modifier token.
/ See \\sa name and \\sa modifier."
      end

